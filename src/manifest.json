{
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "cookies",
    "storage",
    "theme",
    "contextualIdentities",
    "<all_urls>",
    "tabs"
  ],
  "applications": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  },
  "icons": {
    "96": "img/containers.svg"
  },
   "options_ui": {
      "page": "options/options.html",
      "browser_style": true
  },
  "description": "Change theme colour based on your container color",
  "manifest_version": 2,
  "name": "Containers theme",
  "version": "1.0.3"
}
